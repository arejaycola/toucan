		"react-pose": "^4.0.10",
		"react-router-dom": "^5.1.2",
		"react-spring": "^8.0.27",
		"twitter": "^1.7.1",
		"concurrently": "^4.0.1",
		"nodemon": "2.0.2"

	},
	"devDependencies": {
		"concurrently": "^4.0.1"
	},
	"proxy": "http://localhost:5000"
}
